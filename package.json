{
    "name": "voting-dapp",
    "version": "1.0.0",
    "description": "Distributed blockchain-based voting app",
    "main": "app.js",
    "directories": {
        "test": "test"
    },
    "dependencies": {
        "web3": "^1.0.0-beta.36"
    },
    "devDependencies": {
        "@types/bignumber.js": "^5.0.0",
        "@types/chai": "^4.1.7",
        "@types/mocha": "^5.2.5",
        "@types/node": "^10.12.2",
        "@types/web3": "^1.0.13",
        "chai": "^4.2.0",
        "husky": "^1.2.0",
        "mocha": "^5.2.0",
        "truffle-typings": "^1.0.4",
        "ts-node": "^7.0.1",
        "tslint": "^5.11.0",
        "tslint-config-prettier": "^1.17.0",
        "typechain": "^0.3.8",
        "typescript": "^3.1.6"
    },
    "scripts": {
        "test": "truffle test",
        "test-ganache": "truffle test --network ganache",
        "generate": "truffle compile && typechain --target truffle './build/**/*.json'",
        "prettier:write": "prettier --write \"{,!(node_modules|dist|build|coverage)/**/}*.{js,jsx,ts,tsx,json}\"",
        "tslint": "tslint --project tsconfig.json"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run tslint && npm run prettier:write",
            "pre-push": "npm test"
        }
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/pw-mini-inzynierka/voting-dapp.git"
    },
    "keywords": [
        "ethereum",
        "web3",
        "dapp",
        "voting"
    ],
    "author": "Andrzej Wódkiewicz & Gustaw Żyngiel",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/pw-mini-inzynierka/voting-dapp/issues"
    },
    "homepage": "https://github.com/pw-mini-inzynierka/voting-dapp#readme",
    "nyc": {
        "include": [
            "app/*.js"
        ]
    }
}
