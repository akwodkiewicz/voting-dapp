{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "curly": [true, "ignore-same-line"],
        "whitespace": [true],
        "no-trailing-whitespace": [true],
        "max-line-length": [true, 120],
        "ordered-imports": true,
        "no-unused-expression": false,
        "no-unused-variable": true,
        "no-console": false
    },
    "rulesDirectory": []
}